    "lint": "documentation lint src/database.mjs",
    "test": "ava --timeout 2m"
  },
  "dependencies": {
    "leveldown": "^5.4.1",
    "levelup": "^4.3.2"
  },
  "devDependencies": {
    "ava": "^3.3.0",
    "c8": "^7.1.0",
    "documentation": "^12.1.4",
    "semantic-release": "^17.0.2",
    "tmp": "^0.1.0"
  },
  "engines": {
    "node": ">=12.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/k0nsti/konsum-db.git"
  },
  "bugs": {
    "url": "https://github.com/k0nsti/konsum-db/issues"
  },
  "homepage": "https://github.com/k0nsti/konsum-db#readme",
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
