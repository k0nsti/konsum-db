{
  "name": "konsum-db",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/database.mjs",
  "exports": {
    ".": "./src/database.mjs"
  },
  "description": "timeseries database on leveldb",
  "keywords": [
    "leveldb",
    "timeseries"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/database.mjs --section=API",
    "lint": "documentation lint ./src/database.mjs"
  },
  "dependencies": {
    "levelup": "^4.4.0"
  },
  "devDependencies": {
    "ava": "^3.12.1",
    "c8": "^7.3.0",
    "documentation": "^13.0.2",
    "leveldown": "^5.6.0",
    "semantic-release": "^17.1.1",
    "tmp": "0.2.1"
  },
  "engines": {
    "node": ">=12.18.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/k0nsti/konsum-db.git"
  },
  "bugs": {
    "url": "https://github.com/k0nsti/konsum-db/issues"
  },
  "homepage": "https://github.com/k0nsti/konsum-db#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
