{
  "name": "konsum-db",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/master.mjs",
  "exports": {
    ".": "./src/master.mjs"
  },
  "description": "timeseries database on leveldb",
  "keywords": [
    "leveldb",
    "timeseries"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepublishOnly": "publish-please guard",
    "publish-please": "publish-please",
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/master.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/master.mjs"
  },
  "dependencies": {
    "levelup": "^4.4.0"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "leveldown": "^5.6.0",
    "semantic-release": "^17.3.0",
    "tmp": "0.2.1"
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/k0nsti/konsum-db.git"
  },
  "bugs": {
    "url": "https://github.com/k0nsti/konsum-db/issues"
  },
  "homepage": "https://github.com/k0nsti/konsum-db#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github",
      "arlac77/template-github-action"
    ]
  }
}